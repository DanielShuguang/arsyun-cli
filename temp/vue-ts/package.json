{
  "name": "{{ appName }}",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test:unit": "vitest --environment jsdom",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "prettier": "prettier ./src -w",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {}
}